{
    "name": "iv-eddy/wrike-php-sdk",
    "description": "Wrike PHP SDK, configured Wrike PHP Library package for most user cases.",
    "keywords": ["wrike", "wrike.com", "api", "sdk", "library", "guzzle", "client", "REST", "HTTP", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zbigniew Ślązak"
        }
    ],
    "require": {
        "php": ">=5.5,<7.2",
        "zibios/wrike-php-library": "v3.x-dev",
        "zibios/wrike-php-guzzle": "v3.x-dev",
        "iv-eddy/wrike-php-jmsserializer": "master"
        
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/iv-eddy/wrike-php-jmsserializer"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": ">=4.0,<6.0"
    },
    "autoload": {
        "psr-4": {"Zibios\\WrikePhpSdk\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "Zibios\\WrikePhpSdk\\Tests\\": "tests/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
